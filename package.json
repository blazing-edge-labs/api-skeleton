  },
  "dependencies": {
    "@babel/core": "^7.7.4",
    "@babel/preset-env": "^7.7.4",
    "@babel/register": "^7.7.4",
    "babel-plugin-module-resolver": "^3.2.0",
    "bcryptjs": "^2.4.3",
    "connection-string": "^1.0.1",
    "crypto-random-string": "^3.0.1",
    "dotenv-safe": "^8.2.0",
    "is-promise": "^2.1.0",
    "joi": "^14.3.1",
    "jsonwebtoken": "^8.5.1",
    "kcors": "^2.2.2",
    "koa": "^2.11.0",
    "koa-bodyparser": "^4.2.0",
    "koa-compose": "^4.0.0",
    "koa-conditional-get": "^2.0.0",
    "koa-etag": "^3.0.0",
    "koa-helmet": "^5.2.0",
    "koa-jwt": "^3.6.0",
    "koa-mount": "^4.0.0",
    "koa-response-time": "^2.1.0",
    "koa-router": "^7.3.0",
    "lodash": "^4.17.15",
    "migratio": "^2.0.3",
    "minimist": "^1.2.0",
    "nested-error-stacks": "^2.1.0",
    "nodemailer": "^6.4.0",
    "npm-run-all": "^4.1.5",
    "pg-promise": "^10.3.1",
    "random-int": "^2.0.1",
    "slugify": "^1.3.6",
    "toml-require": "^1.2.0"
  },
  "devDependencies": {
    "eslint": "^6.7.2",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "nodemon": "^2.0.1",
    "supertest": "^4.0.2",
    "tap-spec": "^5.0.0",
    "tape": "^4.11.0"
  },
  "engines": {
    "node": "12",
    "yarn": "1"
  },
  "scripts": {
    "ci": "run-s -s lint:check db:recreate db:seed test",
    "db:recreate": "run-s -s \"migrate recreate\"",
    "db:seed": "run-s -s \"migrate seed\"",
    "dev": "run-p -s dev:server",
    "dev:server": "nodemon -q -I -e js,json -i package.json -- -r @babel/register index.js",
    "lint:check": "eslint .",
    "lint:fix": "eslint --fix .",
    "migrate": "node -r @babel/register -r ./env migrate",
    "start": "node -r @babel/register index.js",
    "test": "tape -r @babel/register -r ./env **/*.test.js | tap-spec"
  }
}
